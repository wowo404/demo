package org.liu;import org.liu.pojo.User;import org.liu.pojo.UserRuleRecord;rule "用户年龄小于20或者大于50"     when         $u:User(age < 20 || age > 50);     then         UserRuleRecord record = new UserRuleRecord();         record.setUserId($u.getUserId());         record.setRulePackage(kcontext.getRule().getPackageName());         record.setRuleName(kcontext.getRule().getName());         record.setScore(1); 		insert(record); end